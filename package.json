{
  "name": "tweedle.js",
  "version": "1.1.3",
  "description": "Tweedle.js is a fork of tween.js made to fit my needs.",
  "main": "./dist/tweedle.js",
  "module": "./dist/tweedle.es.js",
  "bundle": "./dist/tweedle.umd.js",
  "types": "./index.d.ts",
  "namespace": "TWEEDLE",
  "files": [
    "dist/",
    "index.d.ts",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/miltoncandelero/tweedle.js.git"
  },
  "bugs": {
    "url": "https://github.com/miltoncandelero/tweedle.js/issues"
  },
  "homepage": "https://miltoncandelero.github.io/tweedle.js/",
  "keywords": [
    "tween",
    "tweening",
    "videogames",
    "typescript",
    "animation"
  ],
  "author": "Milton Candelero <miltoncandelero@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-sucrase": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:src": "eslint src/**/*.ts --fix && eslint test/**/*.ts --fix",
    "lint:test": "eslint src/**/*.ts --fix && eslint test/**/*.ts --fix",
    "build": "run-s bundle types",
    "prebuild": "run-s clean",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf ./dist",
    "clean:types": "rimraf ./compile",
    "clean:docs": "rimraf ./docs",
    "bundle": "rollup -c",
    "types": "tsc --outDir compile --project tsconfig.json && api-extractor run && rimraf compile",
    "prepublishOnly": "run-s test build doc",
    "test": "jest",
    "doc": "typedoc src/index.ts"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.18.17",
    "npm-run-all": "^4.1.5"
  }
}
