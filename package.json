{
  "name": "tweedle.js",
  "version": "0.0.1",
  "description": "Tweedle.js is a fork of tween.js made to fit my needs.",
  "source": "src/index.ts",
  "main": "dist/tweedle.js",
  "files": [
    "dist",
    "types"
  ],
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/miltoncandelero/tweedle.js.git"
  },
  "bugs": {
    "url": "https://github.com/miltoncandelero/tweedle.js/issues"
  },
  "homepage": "https://github.com/miltoncandelero/tweedle.js",
  "keywords": [],
  "author": "Milton Candelero <miltoncandelero@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@pixi/eslint-config": "^1.0.1",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.0",
    "microbundle": "^0.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typedoc": "^0.17.0-3",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts --fix && eslint tests/**/*.ts --fix",
    "build": "rimraf ./dist && rimraf ./types && microbundle --name Tween && tsc ./src/index.ts --emitDeclarationOnly --stripInternal --target es5 -d --outDir ./types",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "doc": "rimraf ./docs && typedoc --mode library --out docs src/index.ts --excludeNotExported --excludePrivate --excludeProtected --stripInternal --theme minimal"
  }
}